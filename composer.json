{
    "name": "firevel/firestore-cache-driver",
    "description": "Firestore cache driver for Laravel app running inside Google App Engine.",
    "keywords": ["laravel", "cache", "driver", "firestore", "app engine", "google cloud", "firevel"],
    "type": "library",
    "license": "MIT",
    "require": {
        "php": "^7.2 || ^8.0",
        "google/cloud-firestore": "^1.14",
        "grpc/grpc": "^1.30",
        "illuminate/contracts": "^5.8 || ^6.0 || ^7.0 || ^8.0",
        "illuminate/support": "^5.8 || ^6.0 || ^7.0 || ^8.0"
    },
    "authors": [
        {
            "name": "Michael Slowik",
            "email": "michael.slowik@firevel.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Firevel\\FirestoreCacheDriver\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Firevel\\FirestoreCacheDriver\\FirestoreCacheServiceProvider"
            ]
        }
    }
}
